<% if @model_property.persisted? %>
    location.reload();
<% else %>
  <% options = {
    url: [current_office, @model_property.data_model, :model_properties],
    model_property: @model_property,
    remote: true
  } %>

  modal_div = document.getElementById('app-modal')

  modal_div.innerHTML = "<%= j render 'form_modal', options%>"
  var app_modal = new bootstrap.Modal(document.getElementById('app-modal'))
<% end %>


