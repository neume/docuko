<% breadcrumb :instance, @instance, current_office %>
<div class="row">
  <div class="col-sm-3 mb-2">
    <h5 class='pt-1 mb-2'> <%= @data_model.name %></h1>
    <div class="card">
      <div class="card-body">
        <% @properties.each do |property| %>
          <div id='property-<%= property.id %>'>
            <%= render 'instance_properties/instance_property', property: property, office: current_office %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-sm-9">
    <%= render 'documents', documents: @documents, office: current_office, instance: @instance %>
  </div>
</div>
