<% breadcrumb :instance, @instance, current_office %>
<h5> <%= @data_model.name %></h1>
<div class="row">
  <div class="col-sm-3">
    <div class="card">
      <div class="card-body">
        <% @properties.each do |property| %>
          <div id='property-<%= property.id %>'>
            <%= render 'instance_properties/instance_property', property: property, office: current_office %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-sm-9">
    <%= render 'documents', documents: @documents, office: current_office %>
  </div>
</div>
